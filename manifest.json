{

  "manifest_version": 2,
  "name": "Piggify",
  "version": "1.0",

  "description": "Translates any text on a website into pig-latin",

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["https://www.mozilla.org/*"],
      "js": ["piggify.js"]
    }
  ]

}
